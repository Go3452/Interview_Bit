public class Solution {
    public String[] fizzBuzz(int A) {
        String []str = new String[A];
        int i=0;
        for(int j=0;j<A;j++)
        {
            i=j+1;
         if(i%3==0 && i%5==0)
          str[j] = "FizzBuzz";
         else if(i%5==0)
          str[j] = "Buzz";
         else if(i%3==0)
          str[j] = "Fizz";
         else
          str[j] = String.valueOf(i);
        }
        return str;
    }
}
