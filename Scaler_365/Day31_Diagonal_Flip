public class Solution {
    public int[][] solve(int[][] A) {
        int m=A.length;
        int n=A[0].length;
        int [][]arr=new int[m][n];
        for(int i=0;i<m;i++)
        {
         for(int j=0;j<n;j++)
         {
          arr[j][i]=A[i][j];
         }
        }
        return arr;
    }
}
