public class Solution {
    public int isPalindrome(String A) {
        String an=
         A = A.replaceAll("[^A-Za-z0-9]+", "");
        A=A.toLowerCase();
        for(int i=0,j=A.length()-1;i<j;i++,j--)
        {
         if(A.charAt(i)!=A.charAt(j))
         return 0;
        }
        return  1;
    }
    }
