public class Solution {
    public int solve(int A, int B) {
        int c = B/A;
        int d = B%A;
        int ans=0;
        if(d==0)
        {
         if(c<5)
          ans = 1;
         else if(c%5==0)
          ans = c/5;
         else
          ans = c/5+1;
        }
        else
        {
         if(c<5)
          ans = 1;
         else
          ans = c/5+1;
        }
        return ans;
    }
}
